        <rdfs:comment>&#39;plant&#39; is here a shorthand for plant kingdom</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
    


