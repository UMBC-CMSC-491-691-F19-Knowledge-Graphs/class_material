    <!-- #Berry -->

    <owl:Class rdf:about="#Berry">
        <rdfs:subClassOf rdf:resource="#FruitingBody"/>
    </owl:Class>
    


    <!-- #CarnivorousPlant -->

    <owl:Class rdf:about="#CarnivorousPlant">
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;plant"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                <owl:someValuesFrom rdf:resource="&AfricanWildlifeOntology1;animal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- #Elephant -->

    <owl:Class rdf:about="#Elephant">
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;herbivore"/>
    </owl:Class>
    


    <!-- #FruitingBody -->

    <owl:Class rdf:about="#FruitingBody">
        <rdfs:subClassOf rdf:resource="#PlantParts"/>
    </owl:Class>
    


    <!-- #Grass -->

    <owl:Class rdf:about="#Grass">
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;plant"/>
    </owl:Class>
    


    <!-- #Impala -->

    <owl:Class rdf:about="#Impala">
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;animal"/>
        <rdfs:comment>An african antelope http://en.wikipedia.org/wiki/Impala</rdfs:comment>
    </owl:Class>
    


    <!-- #Omnivore -->

    <owl:Class rdf:about="#Omnivore">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:someValuesFrom rdf:resource="&AfricanWildlifeOntology1;animal"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:someValuesFrom rdf:resource="&AfricanWildlifeOntology1;plant"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;is-part-of"/>
                                        <owl:someValuesFrom rdf:resource="&AfricanWildlifeOntology1;animal"/>
                                    </owl:Restriction>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;is-part-of"/>
                                        <owl:someValuesFrom rdf:resource="&AfricanWildlifeOntology1;plant"/>
                                    </owl:Restriction>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;animal"/>
        <owl:disjointWith rdf:resource="&AfricanWildlifeOntology1;carnivore"/>
        <owl:disjointWith rdf:resource="&AfricanWildlifeOntology1;herbivore"/>
    </owl:Class>
    


    <!-- #Palmtree -->

    <owl:Class rdf:about="#Palmtree">
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;plant"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has-part"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:complementOf rdf:resource="&AfricanWildlifeOntology1;branch"/>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&AfricanWildlifeOntology1;tree"/>
    </owl:Class>
    


    <!-- #Phloem -->

    <owl:Class rdf:about="#Phloem">
        <rdfs:subClassOf rdf:resource="#PlantParts"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#is-proper-part-of"/>
                <owl:someValuesFrom rdf:resource="#Stem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- #PlantParts -->

    <owl:Class rdf:about="#PlantParts">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#is-proper-part-of"/>
                <owl:someValuesFrom rdf:resource="&AfricanWildlifeOntology1;plant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- #RockDassie -->

    <owl:Class rdf:about="#RockDassie">
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;animal"/>
