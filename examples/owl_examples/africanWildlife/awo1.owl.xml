        <rdfs:comment>They eat grasses and broad-leafed plants</rdfs:comment>
    </owl:Class>
    


    <!-- #Root -->

    <owl:Class rdf:about="#Root">
        <rdfs:subClassOf rdf:resource="#PlantParts"/>
    </owl:Class>
    


    <!-- #Stem -->

    <owl:Class rdf:about="#Stem">
        <rdfs:subClassOf rdf:resource="#PlantParts"/>
    </owl:Class>
    


    <!-- #Twig -->

    <owl:Class rdf:about="#Twig">
        <rdfs:subClassOf rdf:resource="#PlantParts"/>
    </owl:Class>
    


    <!-- #Warthog -->

    <owl:Class rdf:about="#Warthog">
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;animal"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:someValuesFrom rdf:resource="#FruitingBody"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:someValuesFrom rdf:resource="#Grass"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:someValuesFrom rdf:resource="#Root"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:someValuesFrom rdf:resource="&AfricanWildlifeOntology1;animal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Warthogs are Omnivorous http://en.wikipedia.org/wiki/Warthog</rdfs:comment>
    </owl:Class>
    


    <!-- #Xylem -->

    <owl:Class rdf:about="#Xylem">
        <rdfs:subClassOf rdf:resource="#PlantParts"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#is-proper-part-of"/>
                <owl:someValuesFrom rdf:resource="#Stem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- file:/Applications/Protege_4.1_beta/AfricanWildlifeOntology1.owl#animal -->

    <owl:Class rdf:about="&AfricanWildlifeOntology1;animal">
        <owl:disjointWith rdf:resource="&AfricanWildlifeOntology1;plant"/>
    </owl:Class>
    


    <!-- file:/Applications/Protege_4.1_beta/AfricanWildlifeOntology1.owl#branch -->

    <owl:Class rdf:about="&AfricanWildlifeOntology1;branch">
        <rdfs:subClassOf rdf:resource="#PlantParts"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;is-part-of"/>
                <owl:someValuesFrom rdf:resource="&AfricanWildlifeOntology1;tree"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Branches are parts of trees.</rdfs:comment>
    </owl:Class>
    


    <!-- file:/Applications/Protege_4.1_beta/AfricanWildlifeOntology1.owl#carnivore -->

    <owl:Class rdf:about="&AfricanWildlifeOntology1;carnivore">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:allValuesFrom rdf:resource="&AfricanWildlifeOntology1;animal"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:allValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;is-part-of"/>
                                <owl:someValuesFrom rdf:resource="&AfricanWildlifeOntology1;animal"/>
                            </owl:Restriction>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;animal"/>
        <owl:disjointWith rdf:resource="&AfricanWildlifeOntology1;herbivore"/>
        <rdfs:comment>Carnivores are exactly those animals that eat only animals.</rdfs:comment>
    </owl:Class>
    


    <!-- file:/Applications/Protege_4.1_beta/AfricanWildlifeOntology1.owl#giraffe -->

    <owl:Class rdf:about="&AfricanWildlifeOntology1;giraffe">
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;animal"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#Twig"/>
                            <rdf:Description rdf:about="&AfricanWildlifeOntology1;leaf"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="&AfricanWildlifeOntology1;lion"/>
        <rdfs:comment>Giraffes are herbivores, and they eat only leaves.</rdfs:comment>
        <rdfs:comment>they also eat twigs</rdfs:comment>
    </owl:Class>
    


    <!-- file:/Applications/Protege_4.1_beta/AfricanWildlifeOntology1.owl#herbivore -->

    <owl:Class rdf:about="&AfricanWildlifeOntology1;herbivore">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:allValuesFrom rdf:resource="&AfricanWildlifeOntology1;plant"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                        <owl:allValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;is-part-of"/>
                                <owl:someValuesFrom rdf:resource="&AfricanWildlifeOntology1;plant"/>
                            </owl:Restriction>
                        </owl:allValuesFrom>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;animal"/>
        <rdfs:comment>Herbivores are exactly those animals that eat only plants or parts of plants </rdfs:comment>
    </owl:Class>
    


    <!-- file:/Applications/Protege_4.1_beta/AfricanWildlifeOntology1.owl#leaf -->

    <owl:Class rdf:about="&AfricanWildlifeOntology1;leaf">
        <rdfs:subClassOf rdf:resource="#PlantParts"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;is-part-of"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#Twig"/>
                            <rdf:Description rdf:about="&AfricanWildlifeOntology1;branch"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Leaves are parts of branches.</rdfs:comment>
    </owl:Class>
    


    <!-- file:/Applications/Protege_4.1_beta/AfricanWildlifeOntology1.owl#lion -->

    <owl:Class rdf:about="&AfricanWildlifeOntology1;lion">
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;animal"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                <owl:someValuesFrom rdf:resource="#Impala"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&AfricanWildlifeOntology1;eats"/>
                <owl:allValuesFrom rdf:resource="&AfricanWildlifeOntology1;herbivore"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Lions are animals that eat only herbivores.</rdfs:comment>
    </owl:Class>
    


    <!-- file:/Applications/Protege_4.1_beta/AfricanWildlifeOntology1.owl#plant -->

    <owl:Class rdf:about="&AfricanWildlifeOntology1;plant">
        <rdfs:comment>Plants are disjoint from animals.</rdfs:comment>
    </owl:Class>
    


    <!-- file:/Applications/Protege_4.1_beta/AfricanWildlifeOntology1.owl#tasty-plant -->

    <owl:Class rdf:about="&AfricanWildlifeOntology1;tasty-plant">
        <rdfs:subClassOf rdf:resource="&AfricanWildlifeOntology1;plant"/>
        <rdfs:subClassOf>
